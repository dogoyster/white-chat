{
  "name": "white-chat",
  "version": "1.0.0",
  "description": "Code security tool for generating dummy data",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:electron\"",
    "dev:renderer": "webpack serve --mode development",
    "dev:electron": "wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .",
    "build": "npm run build:renderer && npm run build:electron",
    "build:renderer": "webpack --mode production",
    "build:electron": "tsc -p tsconfig.electron.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://dogoyster@github.com/dogoyster/white-chat.git"
  },
  "keywords": [
    "electron",
    "code-security",
    "dummy-data"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/dogoyster/white-chat/issues"
  },
  "homepage": "https://github.com/dogoyster/white-chat#readme",
  "devDependencies": {
    "@types/node": "^24.2.0",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12",
    "html-webpack-plugin": "^5.6.3",
    "path-browserify": "^1.0.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "wait-on": "^8.0.4",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "monaco-editor": "^0.52.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
